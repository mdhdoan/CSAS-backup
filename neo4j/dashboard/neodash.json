            "title": "Matched clusters",
            "query": "CALL apoc.load.jsonParams(\n    \"http://34.118.173.82:8000/create_embeddings\",\n    {method: \"POST\", `Content-Type`: \"application/json\"},\n    apoc.convert.toJson({text: $neodash_free_text})\n) YIELD value AS chunk_embedding_dict\nWITH chunk_embedding_dict['embeddings'] AS chunk_embedding_list\n\tUNWIND  chunk_embedding_list AS chunk_embedding\nWITH chunk_embedding[0] AS chunk, chunk_embedding[1] AS embedding\n    CALL db.index.vector.queryNodes(\"cluster-embeddings\", TOINTEGER($neodash_top_k), embedding)\n\tYIELD node AS cluster, score\nWITH cluster, score\n  WHERE cluster.id <> \"28\" AND score >= TOFLOAT($neodash_threshold)\nRETURN score, cluster.id AS cluster_id, cluster.label AS cluster_label ORDER BY score DESC\n\n\n\n",
